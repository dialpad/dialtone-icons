{
  "name": "@dialpad/dialtone-icons",
  "version": "0.0.17",
  "description": "Dialtone icon library",
  "scripts": {
    "build": "npm run build-icons && npm run build-lib",
    "build-icons": "gulp",
    "build-lib": "vue-cli-service build --target lib --name dialtone-icons --no-clean src/main.js",
    "lint": "vue-cli-service lint",
    "release": "npm install && npm run build && npm publish --tag latest"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "vue": ">=2.6"
  },
  "files": [
    "dist/*.js",
    "dist/svg",
    "dist/icons.json"
  ],
  "main": "dist/dialtone-icons.common.js",
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "del": "^6.1.1",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-debug": "^4.0.0",
    "gulp-json-format": "^2.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-svgmin": "^4.1.0",
    "through2": "^4.0.2"
  },
  "peerDependencies": {
    "vue": ">=2.6"
  },
  "repository": "git@github.com:dialpad/dialtone-icons.git"
}
